{
	"name": "arranjacques/monal",
	"description": "An extensible web application designed to aid web developers in quickly building powerful and bespoke web applications.",
	"keywords": ["monal", "application", "framework", "laravel"],
	"license": "MIT",
	"require": {
		"laravel/framework": "4.1.*"
	},
	"autoload": {
		"classmap": [
			"app/database/migrations",
			"app/database/seeds",
			"app/commands",
			"app/controllers",
			"app/libraries",
			"app/models",
			"app/monal",
			"app/repositories",
			"app/tests/TestCase.php"
		]
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-update-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-create-project-cmd": [
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	},
	"minimum-stability": "stable"
}
